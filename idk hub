-- Scripts:

local function DXTOW_fake_script() -- Main_2.Script 
	local script = Instance.new('Script', Main_2)

	local main = script.Parent.Parent.Parent.MainTabs
	local tps = script.Parent.Parent.Parent.Teleports
	local tools = script.Parent.Parent.Parent.Tools
	
	script.Parent.MouseButton1Click:Connect(function()
		main.Visible = true
		tps.Visible = false
		tools.Visible = false
	end)
end
coroutine.wrap(DXTOW_fake_script)()
