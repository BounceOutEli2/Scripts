local function ASHCOW_fake_script() -- Nimbus.Script 
	local script = Instance.new('Script', Nimbus)

	
	_G.nimb = false
	script.Parent.MouseButton1Click:Connect(function()
		if _G.nimb == false then
			_G.nimb = true
			local Nimbus = Instance.new("Part")
			
			script.Parent.Text = 'Nimbus: on'
			
			Nimbus.Name = "Nimbus"
			Nimbus.Parent = game.Players.LocalPlayer.Character
			Nimbus.Transparency = 1
			Nimbus.CanCollide = false
		elseif _G.nimb == true then
			_G.nimb = false
			script.Parent.Text = 'Nimbus: off'
			game.Players.LocalPlayer.Character.Nimbus:Destroy()
		end
	end)
end
coroutine.wrap(ASHCOW_fake_script)()
local function IHSWR_fake_script() -- DragonGb.Script 
	local script = Instance.new('Script', DragonGb)

	script.Parent.MouseButton1Click:Connect(function()
		local plr = game:GetService("Players").LocalPlayer
		local mouse = plr:GetMouse()
		local dcrush = game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Crush")
		local dthrow = game.Players.LocalPlayer.Backpack:FindFirstChild("Dragon Throw")
	
		mouse.KeyDown:connect(function(key)
			if dcrush then
				if key == "[" then
					game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack["Dragon Crush"])
					game.Players.LocalPlayer.Character["Dragon Crush"]:Activate()
					wait(0.1)
					game.Workspace.Live[plr.Name]["Dragon Crush"].Activator["Flip"]:Destroy()
				elseif key == "]" then
					game.Players.LocalPlayer.Character.Head:Destroy()
				end
			elseif dthrow then
				if key == "[" then
					game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack["Dragon Throw"])
					game.Players.LocalPlayer.Character["Dragon Throw"]:Activate()
					wait(0.1)
					game.Workspace.Live[plr.Name]["Dragon Throw"].Activator["Flip"]:Destroy()
				elseif key == "]" then
					game.Players.LocalPlayer.Character.Head:Destroy()
				end
			end
		end)
	end)
end
coroutine.wrap(IHSWR_fake_script)()
local function YRVN_fake_script() -- Noslow.Script 
	local script = Instance.new('Script', Noslow)

	script.Parent.MouseButton1Click:Connect(function()
		while true do
			wait()
			if game.Players.LocalPlayer.Character:FindFirstChild("Action") then
				game.Players.LocalPlayer.Character:FindFirstChild("Action"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("Attacking") then
				game.Players.LocalPlayer.Character:FindFirstChild("Attacking"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("Using") then
				game.Players.LocalPlayer.Character:FindFirstChild("Using"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("hyper") then
				game.Players.LocalPlayer.Character:FindFirstChild("hyper"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("Hyper") then
				game.Players.LocalPlayer.Character:FindFirstChild("Hyper"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("heavy") then
				game.Players.LocalPlayer.Character:FindFirstChild("heavy"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("KiBlasted") then
				game.Players.LocalPlayer.Character:FindFirstChild("KiBlasted"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("Tele") then
				game.Players.LocalPlayer.Character:FindFirstChild("Tele"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("tele") then
				game.Players.LocalPlayer.Character:FindFirstChild("tele"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("Killed") then
				game.Players.LocalPlayer.Character:FindFirstChild("Killed"):Destroy()
			end
			if game.Players.LocalPlayer.Character:FindFirstChild("Slow") then
				game.Players.LocalPlayer.Character:FindFirstChild("Slow"):Destroy()
			end
		end
	
	end)
end
coroutine.wrap(YRVN_fake_script)()
