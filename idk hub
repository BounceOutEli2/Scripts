local function TBBA_fake_script() -- Toprespawn.Script 
	local script = Instance.new('Script', Toprespawn)

	script.Parent.MouseButton1Click:Connect(function()
		while true do
			wait(0.05)
			if game.Players.LocalPlayer.Character:FindFirstChild("SuperAction") then
				game.Players.LocalPlayer.Character:FindFirstChild("SuperAction"):Destroy()
			end
		end
	end)
end
coroutine.wrap(TBBA_fake_script)()
local function OMOS_fake_script() -- Slotmenu.Script 
	local script = Instance.new('Script', Slotmenu)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Backpack.ServerTraits.ChatStart:FireServer(game.Workspace.FriendlyNPCs["Character Slot Changer"])
		wait(.4)
		game.Players.LocalPlayer.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = "Yes"})
		wait(.4)
		game.Players.LocalPlayer.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = "k"})
	end)
end
coroutine.wrap(OMOS_fake_script)()
local function BKLYW_fake_script() -- Majin.Script 
	local script = Instance.new('Script', Majin)

	_G.majin = false
	script.Parent.MouseButton1Click:Connect(function()
		if _G.majin == true then
			_G.majin = false
		elseif _G.majin == false then
			_G.majin = true
			game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
				if key == "n" then
					game.Players.LocalPlayer.Backpack.ServerTraits.FakeChat:FireServer("majin!")
				end
			end)
		end
	end)
end
coroutine.wrap(BKLYW_fake_script)()
